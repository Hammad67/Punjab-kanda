<%= simple_form_for @party do |form| %>
<%binding.pry%>
  <div class="field">
    <%= form.input :name %>
  </div>

  <div class="field">
    <%= form.input :phone_number %>
  </div>

  <div class="field">
    <%= form.input :address %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
